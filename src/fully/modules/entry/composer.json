{
    "name": "fully/entry-module",
    "type": "yii2-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Cherif BOUCHELAGHEM",
            "email": "cherif.bouchelaghem@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "fully\\modules\\entry\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "fully\\modules\\entry\\tests\\": "tests/" }
    },
    "require": {
        "yiisoft/yii2": "^2.0",
        "fully/base": "dev-master",
        "fully/entry": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
